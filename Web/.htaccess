# .htaccess
RewriteEngine On

# Sửa lại Base URL, quan trọng khi dự án nằm trong thư mục con
RewriteBase /DACS2/Web/

# 1. Bỏ qua các file hoặc thư mục tồn tại
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# 2. Chuyển hướng mọi yêu cầu còn lại đến run.php
RewriteRule ^(.*)$ run.php?url=$1 [L,QSA]