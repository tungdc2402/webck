RewriteEngine On
RewriteBase /DACS2/Web/MVC/

# BỎ QUA TẤT CẢ FILE THẬT (css, js, img, fonts, ...) - DÒNG NÀY QUAN TRỌNG NHẤT
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# Admin route
RewriteRule ^admin/(.*)$ run.php?url=$1 [L,QSA]

# User route
RewriteRule ^(.*)$ index.php?url=$1 [L,QSA]